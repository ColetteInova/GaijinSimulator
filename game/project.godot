]
}

[internationalization]

locale/translations=PackedStringArray("res://assets/values/text.br.translation", "res://assets/values/text.en.translation", "res://assets/values/text.es.translation", "res://assets/values/text.ja.translation")

[layer_names]

2d_render/layer_1="World"
2d_render/layer_2="Player"
2d_render/layer_3="Enemies"
2d_render/layer_4="Collectibles"
2d_render/layer_5="UI"
2d_physics/layer_1="World"
2d_physics/layer_2="Player"
2d_physics/layer_3="Enemies"
2d_physics/layer_4="Collectibles"
2d_physics/layer_5="Hazards"

[physics]

2d/physics_engine="GodotPhysics2D"
2d/default_linear_damp=0.0
2d/sleep_threshold_angular=0.139626

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
viewport/hdr_2d=true
environment/defaults/default_clear_color=Color(0.1, 0.1, 0.1, 1)
2d/snap/snap_2d_transforms_to_pixel=true
