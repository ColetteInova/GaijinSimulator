; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Gaijin Simulator"
config/name_localized={
"pt_BR": "Simulador de Estrangeiro"
}
config/version="0.0.1"
run/main_scene="res://scenes/startup.tscn"
config/features=PackedStringArray("4.5", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

GameSettings="*res://configs/game_settings.gd"
SimpleSettings="*res://addons/simple-settings/simple-settings.gd"
SceneTransition="*res://scenes/utils/scene_transition.tscn"
SaveManager="*res://scripts/save_manager.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"
window/stretch/scale_mode="integer"
window/snap/snap_2d_vertices_to_pixel=false

[editor_plugins]

enabled=PackedStringArray("res://addons/simple-settings/plugin.cfg")

[gui]

theme/default_font_antialiasing=0
theme/custom_font="res://assets/fonts/pixel.ttf"

[input]

ui_accept={
"deadzone": 0.5,
